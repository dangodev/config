{
    "editor.autoClosingBrackets": false,
    "editor.autoIndent": true,
    "editor.cursorStyle": "underline",
    "editor.fontFamily": "Meslo LG M for Powerline,Papyrus",
    "editor.quickSuggestions": {
      "other": false
    },
    "editor.parameterHints": false,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [80],
    "editor.snippetSuggestions": "none",
    "editor.tabSize": 2,
    "eslint.trace.server": "message",
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "html",
      "vue"
    ],
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        ".next/": true,
        ".tmp/": true,
        "**/.DS_Store": true,
        "**/.bundle": true,
        "**/.git": true,
        "**/.sass-cache": true,
        "**/bower_components": true,
        "**/node_modules": true,
        "**/tmp": true
    },
    "keyboard.dispatch": "keyCode",
    "vim.useSystemClipboard": true,
    "window.nativeTabs": true,
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
      "activityBar.background": "#fff",
      "activityBar.foreground": "#434a49",
      "editor.background": "#ffffff",
      "sideBar.background": "#ffffff",
      "sideBar.foreground": "#434a49",
      "statusBar.background": "#fefefe",
      "statusBar.foreground": "#434a49"
    },
    "workbench.colorTheme": "Kay Light",
    "workbench.sideBar.location": "left",
    "vim.disableAnnoyingNeovimMessage": true,
    "workbench.startupEditor": "newUntitledFile",
    "extensions.ignoreRecommendations": true
  }
